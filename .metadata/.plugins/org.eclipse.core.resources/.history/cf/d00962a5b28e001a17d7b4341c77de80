 package sample;

class TvTest {

	String color;
	boolean power;
	int channel;
	
	void power() {
		power = !power;   // true면 false로, false면 true로 변경하는 기능
	}
	void chUp(){
		++channel;
	}
	void chDn(){
		--channel;
		
	}
}

class TvSet{
	
	public static void main(String[] args) {
		
		TvTest t = new TvTest();
		t.channel =7;
		t.chUp();
		System.out.println(t.channel);
		
		
		}

	}


