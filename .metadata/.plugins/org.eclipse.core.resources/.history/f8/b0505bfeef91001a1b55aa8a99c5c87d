package meoko.object;

public class User extends Mco {

	private String name;
	private String pw;
	private int countOfPost;
	
	public void view() {
		
	}
	
	public void set(String id,String name,String pw,int countOfPost) {
		super.set(id);
		this.name=name;
		this.pw=pw;
		this.countOfPost=countOfPost;		
	}
	
	public void get(String id,String name,String pw,int countOfPost) {
		super.get(id);
		name = this.name;
		pw = this.pw;
		countOfPost = this.countOfPost;		
	}
}
