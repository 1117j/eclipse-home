package ex;

public interface ManagerIF {


		//가독성을 위해 상수로 표현 
		//메뉴: 1. 로그인 , 2. 회원가입
	
		int LOG_IN = 1;
		int SIGN_UP = 2;
	
		//로그인 완료 후 메뉴
		int PRINT_MENU = 1;	//목록보기
		int INSERT = 2;		//글쓰기
		int MY_POST = 3;	//내가쓴글보기 
		int USER_INFO = 4;	//회원 정보보기
		
		
		//2.3.1 내가 쓴글 보기
		int MODIFY=1;		//수정	
		int DELETE=2;		//삭제	
		int BACKWARD=3;		//뒤로가기
		
		int SUBMIT = 1;		//수정확인완료
		int RETURN = 2;		//재수정
		
		int YES =1;
		int N0 = 2;
		
		//3.리뷰 목록 보기
		int NEW_POST = 1;
		int SEARCH_POST = 2;
		int EXIT = 4;
		

	}
	
	

