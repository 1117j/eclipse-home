	}
	
	public void SignUpComplete() { // V1_2c_1
		// 회원가입 완료 안내문 출력
	}
	
	public void MainMenu() { // V2
		// 메인 메뉴
	}
	
	public void NewMemNotice() { // V2_1
		// 신규 회원 권한 없음 안내문
	
	}

	public void PostListScreen() { // V2_3
		// 회원이 작성한 게시글 목록 화면
	}
	
	public void PostDetailsScreen() { // V2_3a
		// 글 선택시 화면
		
	}
	
