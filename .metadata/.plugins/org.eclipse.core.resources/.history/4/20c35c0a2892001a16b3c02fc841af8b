	}
	
	public void DeleteCancel() { // V2_3b_2
		// NO 선택 시 삭제취소 문구
	}
	
	public void MyInfoScreen() { // V2_4
		// 회원정보 조회 화면
	}

	public void MyInfoModifyNameInput() { // V2_4_1
