 package sample;

 public class TvTest{		
		public static void main(String[] args) {
			
			Tv t = new Tv();
			t.channel =7;
			t.chUp();
			System.out.println(t.channel);
			
			}

		}

 
class Tv {

	String color;
	boolean power;
	int channel;
	
	void power() {
		power = !power;   // true면 false로, false면 true로 변경하는 기능
	}
	void chUp(){
		++channel;
	}
	void chDn(){
		--channel;
		
	}
}


