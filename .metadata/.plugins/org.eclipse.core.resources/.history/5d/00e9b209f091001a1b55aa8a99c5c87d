package ex;

public class Menu extends Mco {
	
	
	String menuName, storeID, menuPrice;
	
	Menu(String id, 					//1. 아이디
		String menuName, 				//2. 메뉴 이름
		String storeID, 				//3. 가게 ID
		String menuPrice){				//4. 메뉴 가격
		
		super(id);
		this.menuName = menuName;
		this.storeID = storeID;
		this.menuPrice = menuPrice;
	}


}
